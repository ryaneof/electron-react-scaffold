{
  "name": "electron-react-scaffold",
  "version": "0.0.2",
  "description": "Just another scaffold.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ryaneof/electron-react-scaffold.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/gulp debug",
    "dist": "NODE_ENV=development ./node_modules/.bin/gulp compile && ./node_modules/.bin/electron-packager ./compile/ Electron-React-Scaffold --out=dist --ignore=dist --asar --platform=darwin --arch=x64 --overwrite --version=0.30.5",
    "build": "NODE_ENV=production ./node_modules/.bin/gulp compile && ./node_modules/.bin/electron-packager ./compile/ Electron-React-Scaffold --out=dist --ignore=dist --prune --asar --all --overwrite --version=0.30.5"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "engines": {
    "electron": "0.30.5",
    "atom-shell": "0.30.5"
  },
  "dependencies": {
    "director": "~1.2.8",
    "react": "~0.13.3"
  },
  "devDependencies": {
    "babel": "~5.8.21",
    "babelify": "~6.3.0",
    "browserify": "~11.0.0",
    "del": "~2.0.0",
    "electron-connect": "~0.3.0",
    "electron-debug": "~0.2.0",
    "electron-packager": "~5.0.2",
    "electron-prebuilt": "~0.30.5",
    "mkdirp": "~0.5.1",
    "gulp": "~3.9.0",
    "gulp-concat": "~2.6.0",
    "gulp-if": "~1.2.5",
    "gulp-install": "~0.5.0",
    "gulp-live-server": "~0.0.23",
    "gulp-minify-css": "~1.2.0",
    "gulp-sass": "~2.0.1",
    "gulp-uglify": "~1.4.0",
    "vinyl-buffer": "~1.0.0",
    "vinyl-source-stream": "~1.1.0",
    "watchify": "~3.3.1"
  }
}
