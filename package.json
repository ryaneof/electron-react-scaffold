{
  "name": "electron-react-scaffold",
  "version": "0.2.0",
  "description": "Just another scaffold.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ryaneof/electron-react-scaffold.git"
  },
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/gulp debug",
    "dist": "NODE_ENV=development ./node_modules/.bin/gulp compile && ./node_modules/.bin/electron-packager ./compile/ Electron-React-Scaffold --out=dist --ignore=dist --asar --platform=darwin --arch=x64 --overwrite --version=0.35.0",
    "build": "NODE_ENV=production ./node_modules/.bin/gulp compile && ./node_modules/.bin/electron-packager ./compile/ Electron-React-Scaffold --out=dist --ignore=dist --prune --asar --all --overwrite --version=0.35.0"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "engines": {
    "electron": "0.35.0",
    "atom-shell": "0.35.0"
  },
  "dependencies": {
    "babel": "^6.1.18",
    "director": "^1.2.8",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "run-sequence": "^1.1.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babelify": "^7.1.0",
    "browserify": "^12.0.1",
    "del": "^2.1.0",
    "electron-connect": "^0.3.3",
    "electron-debug": "^0.5.0",
    "electron-packager": "^5.1.0",
    "gulp": "^3.9.0",
    "gulp-if": "^2.0.0",
    "gulp-install": "^0.6.0",
    "gulp-live-server": "0.0.29",
    "gulp-minify-css": "^1.2.0",
    "gulp-sass": "^2.0.4",
    "gulp-uglify": "^1.5.1",
    "mkdirp": "^0.5.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.1"
  }
}
